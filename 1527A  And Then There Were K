#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    int t,x;
    cin >> t;
    for (int tt = 0; tt < t; tt++)
    {
        int n;
        cin>>n;
 
         x= 1;
 
        while (true)
        {
            if(x*2>n)
            {
                cout<<x-1<<"\n";
                break;
            }
            x *= 2 ;
        }
    }

    return 0;
}
