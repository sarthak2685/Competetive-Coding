#include<bits/stdc++.h>
using namespace std;
long long t,a,b;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);cout.tie(0);
    cin>>t;
    while(t--)
    {
        cin>>a>>b;
        if(a==b) {
            cout<<0<<endl;
            continue;
        }
        long long num=10;
        long long num1=max(a,b)-min(a,b);
        long long ans=0;
        long long ss=num1/10;
        ans+=ss;
        if(num1%10) cout<<ans+1<<endl;
        else cout<<ans<<endl;
    }
    return 0;
 } 

