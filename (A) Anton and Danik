#include<bits/stdc++.h>
using namespace std;
int main()
{
   long long int n;
   int count=0,a=0;
   cin>>n;
   string s;
   cin>>s;
   for(int i=0;i<n;i++){
       if(s[i]=='A') count++;
       else a++;
   }
   if(count>a) cout<<"Anton";
   else if(a>count) cout<<"Danik";
   else cout<<"Friendship";
  
    return 0;
}
