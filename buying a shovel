#include<bits/stdc++.h>
using namespace std;
int main(){
    int k,r;
    cin>>k>>r;
   for(int i=1;i<10;i++){
    int p;
    p=k*i;
    if(p%10==0){
        cout<<i<<endl;
        break;
    }
    else{
        p=p-r;
        if(p%10==0){
            cout<<i<<endl;
            break;
        }
    }
}
}
